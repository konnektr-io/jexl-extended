{
  "name": "jexl-extended",
  "version": "1.2.23",
  "description": "Extended grammar for Javascript Expression Language (JEXL)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "readme": "README.md",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc -p tsconfig.release.json",
    "docs:completion": "typedoc && ts-node ./completion-docs.ts",
    "docs:html": "ts-node ./docs.ts",
    "docs": "npm run docs:completion && npm run build && npm run docs:html"
  },
  "keywords": [
    "JSON",
    "expression",
    "evaluator",
    "parser",
    "target",
    "context",
    "jexl",
    "filter",
    "selector"
