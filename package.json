{
  "name": "jexl-extended",
  "version": "1.3.7",
  "description": "Extended grammar for Javascript Expression Language (JEXL)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "readme": "README.md",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc -p tsconfig.release.json",
    "docs:typedoc": "typedoc --json ./docs/typedoc.json --entryPoints ./src/extended-grammar.ts",
    "docs:completion": "ts-node ./completion-docs.ts",
    "docs:json": "ts-node ./docs.ts",
    "docs:mdx": "ts-node ./generate-mdx-docs.ts",
    "docs:mdx:clean": "ts-node ./mdx-utils.ts clean",
    "docs:mdx:validate": "ts-node ./mdx-utils.ts validate",
    "docs:mdx:stats": "ts-node ./mdx-utils.ts stats",
    "docs": "npm run docs:typedoc && npm run docs:completion && npm run build && npm run docs:json && npm run docs:mdx"
  },
  "keywords": [
    "JSON",
    "expression",
    "evaluator",
    "parser",
    "target",
    "context",
    "jexl",
    "filter",
    "selector"
  ],
  "author": "Niko Raes <niko.raes@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnektr-io/jexl-extended.git"
  },
  "homepage": "https://docs.konnektr.io/docs/jexl",
  "license": "MIT",
  "peerDependencies": {
    "monaco-editor": ">=0.40.0"
  },
  "peerDependenciesMeta": {
    "monaco-editor": {
      "optional": true
    }
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "jexl": "^2.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/jexl": "^2.3.4",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.5.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.3.0",
    "typedoc": "^0.28.9",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.44.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}